@model  List<mingmar4thsem.Models.person>

@{
    ViewBag.Title = "inputPerson";
    Layout = "~/Views/Shared/_Layout.cshtml";
}

<h2>Enter person</h2>
<table class="tbl" border="1">
    <tr>
        <th>Sn</th>
        <th>Name</th>
        <th>Address</th>
        <th>Contact</th>
    </tr>

    @foreach(var data in Model)
            {
                <tr>
                    <td>@data.id</td>
                    <td>@data.name</td>
                    <td>@data.address</td>
                    <td>@data.contact</td>
                </tr>
                
            }
</table>

